web1: 
  name: transport
  document_root: web 
  shared_writable_dirs:
    - var
  php_extensions:
    - curl
    - xhprof
  after_build:
    - "mv /var/www/config/pagodabox.php /var/www/config/local.php"
    - "curl -s http://getcomposer.org/installer | php"
    - "php composer.phar install"

cache1:
  name: stats
  type: redis
